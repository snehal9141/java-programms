
public class Main
{
	public static void main(String[] args) {
	    int[]a={2,7,4,3,6};
	    int[]b={3,8,6,8,2};
		System.out.println("Duplicate elements in given array");
		for(int i=0;i<a.length;i++){
		    for(int j=0;j<b.length;j++){
		        if(a[i]==b[j])
		        System.out.println("Number: "+a[i]+"found repeating at position:"+i+"  ,Repeted at position"+j);
		    }
		}
	}
}
